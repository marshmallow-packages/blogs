{
    "name": "marshmallow/blogs",
    "description": "This packages add the resources you need to create a simple blog in Nova. This is fully extendable.",
    "keywords": [
        "marshmallow",
        "laravel blog",
        "nova blog"
    ],
    "homepage": "https://github.com/Marshmallow-Development/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "stef@marshmallow.dev"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "marshmallow/sluggable": "^v1.4.0",
        "marshmallow/helpers": "^v2.14.0",
        "marshmallow/commands": "^v1.2.1",
        "marshmallow/nova-tinymce": "^v2.1.1",
        "marshmallow/translatable": "^v2.0.9",
        "marshmallow/redirectable": "^v2.0.0",
        "marshmallow/seoable": "^v3.0.2",
        "marshmallow/nova-flexible": "^v2.2.2",
        "marshmallow/nova-advanced-image": "^v2.0.0",
        "marshmallow/breadcrumb": "^v2.0.1"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Blogs\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Blogs\\BlogsServiceProvider"
            ],
            "aliases": {
                "Blog": "Marshmallow\\Blogs\\Facades\\Blog"
            }
        }
    }
}
