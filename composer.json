{
    "name": "marshmallow/blogs",
    "description": "This packages add the resources you need to create a simple blog in Nova. This is fully extendable.",
    "keywords": [
        "marshmallow",
        "laravel blog",
        "nova blog"
    ],
    "homepage": "https://github.com/Marshmallow-Development/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "stef@marshmallow.dev"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "marshmallow/sluggable": "^1.0",
        "marshmallow/helpers": "^2.0",
        "marshmallow/commands": "^1.0",
        "marshmallow/nova-tinymce": "^1.0",
        "marshmallow/translatable": "^1.0",
        "marshmallow/redirectable": "^1.0",
        "marshmallow/seoable": "^2.3",
        "marshmallow/nova-flexible": "^1.0",
        "marshmallow/nova-advanced-image": "^1.0 || ^1.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Blogs\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Blogs\\BlogsServiceProvider"
            ],
            "aliases": {
                "Blog": "Marshmallow\\Blogs\\Facades\\Blog"
            }
        }
    }
}
